package pe.com.techniser.microservices.limitsservice.controller;

public class Limits {
	
	private int minimun;
	private int maximum;
	
	public Limits() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Limits(int minimunl, int maximum) {
		super();
		this.minimun = minimunl;
		this.maximum = maximum;
	}
	public int getMinimunl() {
		return minimun;
	}
	public void setMinimunl(int minimunl) {
		this.minimun = minimunl;
	}
	public int getMaximum() {
		return maximum;
	}
	public void setMaximum(int maximum) {
		this.maximum = maximum;
	}

}
